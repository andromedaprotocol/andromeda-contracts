            ],
            "properties": {
              "at_time": {
                "$ref": "#/definitions/Milliseconds"
              }
            },
            "additionalProperties": false
          }
        ]
      },
      "Milliseconds": {
        "description": "Represents time in milliseconds.",
        "type": "integer",
        "format": "uint64",
        "minimum": 0.0
      },
      "OwnershipMessage": {
